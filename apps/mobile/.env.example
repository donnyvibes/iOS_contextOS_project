# iOS ContextOS Mobile App Environment Variables

# App Configuration
APP_NAME=iOS ContextOS Mobile
APP_VERSION=1.0.0
APP_ENVIRONMENT=development

# API Configuration
API_BASE_URL=https://api.example.com
API_VERSION=v1
API_TIMEOUT=30000

# Authentication
AUTH_CLIENT_ID=your_client_id_here
AUTH_CLIENT_SECRET=your_client_secret_here
AUTH_REDIRECT_URI=contextOS://auth/callback

# Location Services
LOCATION_ENABLED=true
LOCATION_ACCURACY=high

# Push Notifications
PUSH_NOTIFICATIONS_ENABLED=true
APNS_KEY_ID=your_apns_key_id
APNS_TEAM_ID=your_team_id

# Analytics
ANALYTICS_ENABLED=true
ANALYTICS_TRACKING_ID=your_tracking_id

# Debug Settings
DEBUG_MODE=true
LOG_LEVEL=debug

# Context Awareness
CONTEXT_UPDATE_INTERVAL=5000
CONTEXT_CACHE_DURATION=300000
