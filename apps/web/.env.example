# Development Environment Variables
# Copy this file to .env and fill in your values

# Vite Environment
VITE_APP_NAME=ContextOS Web
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=development

# API Configuration
VITE_API_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# Database Configuration  
VITE_DATABASE_URL=your_database_url_here

# Authentication
VITE_AUTH_SECRET=your-super-secret-auth-key-here
VITE_NEXTAUTH_URL=http://localhost:5173

# External Services
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_PWA=false

# Storage
VITE_STORAGE_PROVIDER=local
VITE_S3_BUCKET=your_s3_bucket_here
VITE_S3_REGION=us-east-1

# WebSocket Configuration
VITE_WS_URL=ws://localhost:3001

# Logging
VITE_LOG_LEVEL=debug
